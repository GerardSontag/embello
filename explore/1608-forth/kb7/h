\ install hardware abstractions in flash

cr eraseflash
compiletoflash

8 constant io-ports  \ A..H

include ../mlib/hexdump.fs
include ../flib/io-stm32f4.fs
include ../flib/hal-stm32f7.fs

include clock.fs

: initx ( -- )  \ board initialisation
  216Mhz
;

reset
